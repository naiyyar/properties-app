<div style="padding: 10px 10px 0px 20px;">
  <div class="pull-left">
    <% if user_signed_in? %>
      <%= link_to 'Add Photo', new_building_upload_path(building_id: object.id, upload_type: 'photos'), class: 'btn btn-o btn-primary btn-sm' %>
    <%else%>
      <%= link_to 'Add Photo', '#signin', 'data-toggle' => 'modal', class: 'btn btn-o btn-primary btn-sm'  %>
    <%end%>  
  </div>

  <div class="see_all pull-right">
    <span class="fa fa-th-large"></span> 
    <% if object.kind_of? Building %>
      <%= link_to "See all(#{object.uploaded_images_count})", building_uploads_path(building_id: object.id), "data-no-turbolink" => false %>
    <%else%>
      <%= link_to "See all(0)", unit_uploads_path(unit_id: object.id), "data-no-turbolink" => false %>
    <%end%>
  </div>
</div>