<!DOCTYPE html>
<html>
  <head>
    <title>AptreviewApp</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <script src="//maps.google.com/maps/api/js?v=3.23&key=AIzaSyDZtWN-GlVsIjuQMP8xN5dit3PmlMb-qjA"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <%= render 'layouts/shared/navbar' %>
    <div class="container">
      <%= render partial: 'layouts/shared/flash_messages', flash: flash  %>
      <%= yield %>
      <br />
    </div>
    <%= render partial: "layouts/shared/login_modal" %>

    <script type="text/javascript">
      $('#slider').rotateSlider({});
      $(function() {
        new app.apartments;
      });
   
    </script>

  </body>
</html>
