      font-family: "Source Sans Pro", Helvetica, Arial, sans-serif;

  %body{style: 'background-color: #fff !important;', class: 'no-hidden no-touch' }
    = render 'layouts/home/header'
    = render 'layouts/shared/left_side'
    = yield
    %br
    = render 'layouts/home/footer'

    = javascript_include_tag 'home' 
    = javascript_include_tag 'zapp' 
