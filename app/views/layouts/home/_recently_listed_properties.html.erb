<h2 class="osLight">Recently Listed Properties</h2>
<div class="row pb40">
  <% if @recently_listed_properties.present? %>
    <% @recently_listed_properties.each do |building| %>
      <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
        <a href="<%=building_path(building)%>" class="propWidget-2">
          <div class="fig">
            <% if building.image_uploads.present? %>
              <%= image_tag building.image_uploads.last.image, alt: 'image', style: "width: 353px; height: 210px;" %>
              <%= image_tag building.image_uploads.last.image, alt: 'image', class: 'blur', style: "width: 353px; height: 210px;" %>
            <%else%>
              <%= image_tag 'prop/1-1.png', alt: 'image' %>
              <%= image_tag 'prop/1-1.png', alt: 'image', class: 'blur' %>
            <%end%>
            <div class="opac"></div>
            <!-- <div class="priceCap osLight"><span>$1,750,000</span></div> -->
            <!-- <div class="figType">FOR SALE</div> -->
            <h3 class="osLight"><%= building.building_name_or_address %></h3>
            <div class="address"><%= building.street_address %>, <%= building.zipcode %></div>
            <!-- <ul class="rating"> -->
              <%#= rating_for building, "building", disable_after_rate: true,readonly: true, cancel: false %>
            <!-- </ul> -->
          </div>
        </a>
      </div>
    <%end%>
  <%end%>
</div>