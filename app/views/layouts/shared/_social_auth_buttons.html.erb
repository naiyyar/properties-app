<% if button_type == 'google' %>
	<div class="google-auth-icon">
		<svg aria-hidden="true" class="svg-icon" width="18" height="18" viewBox="0 0 18 18"><g><path d="M16.51 8H8.98v3h4.3c-.18 1-.74 1.48-1.6 2.04v2.01h2.6a7.8 7.8 0 0 0 2.38-5.88c0-.57-.05-.66-.15-1.18z" fill="#4285F4"></path><path d="M8.98 17c2.16 0 3.97-.72 5.3-1.94l-2.6-2a4.8 4.8 0 0 1-7.18-2.54H1.83v2.07A8 8 0 0 0 8.98 17z" fill="#34A853"></path><path d="M4.5 10.52a4.8 4.8 0 0 1 0-3.04V5.41H1.83a8 8 0 0 0 0 7.18l2.67-2.07z" fill="#FBBC05"></path><path d="M8.98 4.18c1.17 0 2.23.4 3.06 1.2l2.3-2.3A8 8 0 0 0 1.83 5.4L4.5 7.49a4.77 4.77 0 0 1 4.48-3.3z" fill="#EA4335"></path></g></svg>
	</div>
	<a href="<%=user_google_oauth2_omniauth_authorize_path%><%#=omniauth_authorize_path(resource_name, :google_oauth2)%>">
	  <div class="tr-auth-btn google-login">
	    <div style="display: inline-flex;justify-content: center;"><%=btn_title%></div>
	  </div>
	</a>
<%else%>
	<div class="fb-auth-icon">
		<svg xmlns="https://www.w3.org/2000/svg" viewBox="0 0 216 216" class="_5h0m" color="#FFFFFF"><path fill="#FFFFFF" d="M204.1 0H11.9C5.3 0 0 5.3 0 11.9v192.2c0 6.6 5.3 11.9 11.9
    11.9h103.5v-83.6H87.2V99.8h28.1v-24c0-27.9 17-43.1 41.9-43.1
    11.9 0 22.2.9 25.2 1.3v29.2h-17.3c-13.5 0-16.2 6.4-16.2
    15.9v20.8h32.3l-4.2 32.6h-28V216h55c6.6 0 11.9-5.3
    11.9-11.9V11.9C216 5.3 210.7 0 204.1 0z"></path></svg>
    <%= image_tag 'fb-logo.png', width: "18", height: "18", class: 'invisible' %>
	</div>
	<a href="<%=omniauth_authorize_path(resource_name, :facebook)%>">
	  <div class="tr-auth-btn fb-auth-button">
	    <div style="display: inline-flex;justify-content: center;"><%=btn_title%></div>
	  </div>
	</a>
<%end%>