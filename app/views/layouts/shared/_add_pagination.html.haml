- if false
	- if mobile?
		= will_paginate objects, renderer: WillPaginate::ActionView::BootstrapLinkRenderer, previous_label: '← Prev', next_label: 'Next →', inner_window: 0, outer_window: 0, :page_links => false, list_classes: ["pagination #{last_page_class(objects)} #{first_page_class(objects)}"]
	- else
		= will_paginate objects, renderer: WillPaginate::ActionView::BootstrapLinkRenderer, previous_label: '← Prev', list_classes: %w(pagination justify-content-center)