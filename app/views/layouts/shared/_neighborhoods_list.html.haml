.col-xs-12.col-sm-6.col-md-3.col-lg-3
  .pop-list
    - area = 'MANHATTAN'
    %b#lower_manhattan
      = search_by_neighborhood_link('Lower Manhattan', area)
    .child-borough-container
      - NYCBorough.lower_manhattan_sub_borough.each do |borough|
        - if borough != 'West Village'
          .parent{'data-parent' => 'lower_manhattan'}
            = search_by_neighborhood_link(borough, area)
            - if borough == 'Greenwich Village'
              %ul
                %li{'data-parent' => 'lower_manhattan'}= search_by_neighborhood_link('West Village', area)
    %b#midtown= search_by_neighborhood_link('Midtown', area)
    .child-borough-container
      .parent{'data-parent' => 'midtown'}= search_by_neighborhood_link('Chelsea', area)
      .parent{'data-parent' => 'midtown'}= search_by_neighborhood_link('Flatiron District', area)
      .parent{'data-parent' => 'midtown'}= search_by_neighborhood_link('Gramercy Park', area)
      .parent{'data-parent' => 'midtown'}= search_by_neighborhood_link("Hell's Kitchen", area)
      .parent{'data-parent' => 'midtown'}= search_by_neighborhood_link('Kips Bay', area)

.col-xs-12.col-sm-6.col-md-3.col-lg-3
  .pop-list
    .child-borough-container
      .parent{'data-parent' => 'midtown'}
