- if object.present?
	- if object.kind_of?(FeaturedListing)
		.col-12.col-sm-12.col-md-6.col-lg-6
			= render 'featured_listings/detail_card', featured_listing: object
	- elsif object.kind_of?(FeaturedAgent)
		= render 'featured_agents/agent', agent: object
	- else
		.col-12.col-sm-12.col-md-6.col-lg-6
			= render 'buildings/detail_card', building: object