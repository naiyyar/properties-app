.contact-buttons
	.row
		- if @building.show_apply_link? and @building.web_url.present? and @building.show_contact_leasing?
			.col-lg-6{style: 'padding-right: 0px;'}= check_availability_button(@building.web_url, 'btn-o font-bolder btn-block')
			.col-lg-6= apply_online_link(@building)
			.col-lg-12.margin-top-10= contact_leasing_link(@building)
		- elsif @building.show_apply_link? and @building.web_url.present?
			.col-lg-12= apply_online_link(@building)
			.col-lg-12.margin-top-10= check_availability_link(@building)
		- elsif @building.web_url.present? and @building.show_contact_leasing?
			.col-lg-12= check_availability_link(@building)
			.col-lg-12.margin-top-10= contact_leasing_link(@building)
		- elsif @building.show_apply_link? and @building.show_contact_leasing?
			.col-lg-12= apply_online_link(@building)
			.col-lg-12.margin-top-10= contact_leasing_link(@building)
			