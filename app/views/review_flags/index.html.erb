<p id="notice"><%= notice %></p>

<h1>Listing Review Flags</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @review_flags.each do |review_flag| %>
      <tr>
        <td><%= link_to 'Show', review_flag %></td>
        <td><%= link_to 'Edit', edit_review_flag_path(review_flag) %></td>
        <td><%= link_to 'Destroy', review_flag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review flag', new_review_flag_path %>
