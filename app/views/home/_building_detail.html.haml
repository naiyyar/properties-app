- if @management_company.present? and controller_name == 'management_companies'
	%div{style: 'padding: 0px 0px 0px 11px;height: 22px;'}
		= "#{building.building_type} in" if building.building_type.present?
		= link_to building.neighborhood_name, borough_search_link(building.neighbohoods), class: 'hyper-link nb-link' if building.neighbohoods.present?

.clearfix