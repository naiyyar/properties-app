var section = $('#featured-buildings-section');
<% unless @mobile_view %>
    section.html("<%= j render partial: 'layouts/home/featured_buildings' %>");
<%else%>
    section.html("<%= j render partial: 'layouts/shared/slick_slider' %>");
    $(".center").slick({
    dots: false,
    infinite: true,
    centerMode: true,
    slidesToShow: 1,
    slidesToScroll: 1,
    arrows: false
  });
<%end%>

$('#home-popular_neighborhoods').html("<%= j render partial: 'layouts/home/popular_neighborhoods' %>");
$('#home-popular_searches').html("<%= j render partial: 'layouts/home/popular_searches', locals: { bg_col: '#FBFBFF' } %>");