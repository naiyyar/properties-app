<%-if params[:type] == 'email' %>
	$('#remote-container').html("<%=j render 'billings/invoice_email_form' %>");
	$('#emailInvoice').modal('show');
	$('[data-bs-toggle="tooltip"]').tooltip();
<%else%>
	$('#remote-container').html("<%=j render 'billings/preview_invoice' %>");
	$('#showInvoice').modal('show');
<%end%>